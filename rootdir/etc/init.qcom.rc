on property:log.tag.rpmdebug=3
	start prd_modify_vddq

on property:log.tag.rpmdebug=4
	start prd_fix_384M

on property:log.tag.rpmdebug=5
	start prd_modify_s5_1424

on property:log.tag.rpmdebug=6
	start prd_diasble_pbs

on property:log.tag.rpmdebug=7
	start prd_modem_phy_addr

on property:log.tag.rpmdebug=8
	start prd_rpm_mode_clean

on property:adbd.adb.nvitem=*
    start prd_enadb_func

on property:log.tag.audio_arg=*
	start prd_audio_arg

on property:log.tag.diskinf=1
	start prd_disk_info

on property:persist.sys.debugopt.pkgname=*
      start prd_opt_odex

on property:log.tag.file_clean=*
	start prd_file_clean


on property:log.tag.kerneldump=1
     write /sys/class/power_supply/battery/target_info 68

on property:log.tag.s2reset=1
    write /sys/module/msm_poweroff/parameters/s2_reset_mode 1
on property:log.tag.s2reset=0
    write /sys/module/msm_poweroff/parameters/s2_reset_mode 0
# Logcat dump daemon, dumps logs to logdump partition
#service logdumpd /system/bin/logcat -b all -v threadtime -D -w /dev/block/bootdevice/by-name/logdump
