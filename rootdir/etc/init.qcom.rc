#open collect user modem ramdump
on property:log.tag.modemdump=1
    setprop log.tag.dump_oem 1
    setprop persist.sys.ssr.restart_module modem
    setprop persist.sys.ssr.xtc_level system

#close cllect user modem ramdump
on property:log.tag.modemdump=0
    setprop log.tag.modem_clr_oem 1
    setprop persist.sys.ssr.restart_module modem
    setprop persist.sys.ssr.xtc_level related

on property:persist.sys.wifiadb=1
    setprop service.adb.tcp.port 5555
    stop adbd
    start adbd

# add by ouyangfan on 2020/12/31 for xtcdiag wifi rf test.(qcom and sprd platform is different)
# it`s use in {@link frameworks/base/core/java/com/xtc/xtcdiag/wifi/WifiUtil.java}
# log.insmod.wifi.driver=1 load wifi ftm mode, log.insmod.wifi.driver=0 rmmod wlan driver.
on property:log.insmod.wifi.driver=1
    #insmod /vendor/lib/modules/pronto_wlan.ko con_mode=5

on property:log.insmod.wifi.driver=0
    #rmmod wlan
    start ptt_rmmod_wifi

service e4defrag /system/bin/e4defrag -d /data /data
    user root
    disabled
    oneshot
    seclabel u:r:xtc_init:s0

on property:sys.flash.e4defrag=true
    start e4defrag

